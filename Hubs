local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("More Hubs Blox Fruits by 5waylie", "Sentinel")

local Tab = Window:NewTab("Ren Hub")

local Section = Tab:NewSection("Hub")

Section:NewButton("Script", "ButtonInfo", function()
    print("Clicked")
loadstring(game:HttpGet('https://raw.githubusercontent.com/SHAREHACK/bloxfruit/main/free'))()
end)

